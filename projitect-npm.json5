/*
 * Set the NPM_TOKEN correctly for repos that need to install packages from
 * the @projitect Github packages registry
 */

{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",

  // Set the key for accessing Github packages
  "encrypted": {
    "npmToken": ""
  },
  "npmrc": "//npm.pkg.github.com/:_authToken=${NPM_TOKEN}\n@projitect:registry=https://npm.pkg.github.com"
}
